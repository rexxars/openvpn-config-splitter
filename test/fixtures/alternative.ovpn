# Set up IP tunnel client over UDP against the listed servers
client
proto udp
dev tun
remote foo-bar.zing.io 1194
resolv-retry infinite

# Drop privileges and attempt to persist some state across restarts
nobind
user nobody
group nogroup
persist-key
persist-tun

# Set up connection
remote-cert-tls server
cipher AES-192-CBC
key-direction 1 # Must be '0' on server (see tls-auth)
comp-lzo

# Use authentication, and disable cache and renegotiation due to Google Authenticator
auth-user-pass
auth-nocache
reneg-sec 0

# Be more verbose
verb 3

