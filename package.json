  "author": {
    "name": "Espen Hovlandsdal",
    "email": "espen@hovlandsdal.com",
    "url": "http://rexxars.com"
  },
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "ovpnsplit": "lib/cli.js"
  },
  "main": "lib/splitter.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/rexxars/openvpn-config-splitter.git"
  },
  "scripts": {
